{
  "cleanUrls": false,
  "trailingSlash": false,
  "outputDirectory": "public",
  "headers": [
    {
      "source": "/mobileconfig/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-apple-aspen-config"
        }
      ]
    },
    {
      "source": "/.well-known/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ],
  "redirects": [],
  "rewrites": [],
  "ignoreCommand": "echo 'Skip Vercel Build'"
}
